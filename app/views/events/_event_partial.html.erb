<div class="sidbar_tables">
	<h2>Upcoming Events</h2>
	<% @events_partial.each do |e| %>
		<div class="item">
			<ul class="table_row">
				<%= content_tag :li, e.title, class: "title" %>
				<li><strong>When:</strong> <%= e.event_start %></li>
				<li><strong>Where:</strong> <%= e.location %></li>
				<%= content_tag :li, e.body, class: "message" %>
				
			</ul>
			<br class="clear">
		</div>
	<% end %>
</div>

