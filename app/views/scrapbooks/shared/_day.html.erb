<div class="notice"></div>
<% ENV.each do |key, value| %>
<p><%= key %> => <%= value%></p>
<% end %>
<%= render('javascript/image') %>

<!-- Go To Page link button with a bread crumb -->
<% image_num = 0 %>
<div id="scrapbook_template">
	<%= render('scrapbooks/shared/'+page+'_images') %>
</div>


<%= render('scrapbooks/shared/up_down_prev_next_links') %>
<%= render('shared/journal_script') %>

<!-- Nav_menu toggling through form submission changing nav_menu boolean -->
<%= form_for @user, remote: true, html: {class: "nav_bar_viz"} do |f| %>
		<% f.check_box :nav_menu %>
<% end %>

<div>
	<div class="nav_btns delete_nav_btn nav_btn_style"> - - - </div>
	<div class="nav_btns gray_nav_btn nav_btn_style"> - - - </div>
</div>
<!-- end nav_menu toggling -->

<div class="time_navigation">		
	<%= render('menus/thumbnails') %>
	<%= render('menus/by_day') %>
	<%= render('menus/by_week', :beg_month => @first_of_month ) %>
	<%= render('menus/by_month', :beg_range => @first_of_month ) %>
</div>


