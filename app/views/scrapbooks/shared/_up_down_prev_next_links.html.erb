<!-- Determines the next time period. ie. if on day page, then will create: "Go to Week" link in menu -->  

<%= link_to content_tag(:div, go_to_text("up"), class: "nav_up absolute_withZ"), go_to_up_link(@user)+up_link_params(@user), remote: true %>

<%= link_to content_tag(:div, "", class: "nav_up2 absolute_withZ"), go_to_up_link(@user)+up_link_params(@user), remote: true %>


<%= link_to content_tag(:div, content_tag(:div, "back", class: "nav_text"), class: "nav_down absolute_withZ"), go_to_down_link(@user)+down_link_params(@user), remote: true unless page == "day" %>


<div class="nav_arrows">
	
    <%= link_to content_tag(:div, content_tag(:div, prev_next_link_text("prev", @user), class: "nav_text"), class: "prev prev-next direction absolute_withZ"), go_to_date_link(@user)+prev_link_params(@user), remote: true %>

    <%= link_to content_tag(:div, content_tag(:div, prev_next_link_text("next", @user), class: "nav_text"), class: "next prev-next direction absolute_withZ"), go_to_date_link(@user)+next_link_params(@user), remote: true %>
</div>
