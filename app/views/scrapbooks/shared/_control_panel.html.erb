<li class="resize_msg cp" data-id="<%= image.id %>" data-range="<%= page %>" data-root= "<%= root_path %>" data-purpose="Resize your image"></li>
<%= content_tag :li, "", class: "backward z_index cp change_z_#{image.id}", :"data-root" => root_path, :"data-id" => image.id, :"data-purpose" => "push image back", :"data-z" => @z_index, :"data-range" => page %>

<%= content_tag :li, "", class: "forward z_index cp change_z_#{image.id}", :"data-root" => root_path, :"data-id" => image.id, :"data-purpose" => "bring image forward", :"data-z" => @z_index, :"data-range" => page %>
<!-- <li class="crop_msg cp" data-crumb=" <%= @bread_crumb %> " data-id="<%= image.id %>" data-range="<%= page %>" data-root= "<%= root_path %>">Crop</li> -->
<li><%= link_to "", [:edit, @user, image], class: "edit_msg cp", :"data-root" => root_path, :"data-id" => image.id, :"data-purpose" => "Edit your image", remote: true %></li>




<li class="message cp"></li>
