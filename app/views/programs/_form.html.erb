<%= form_for @program do |f| %> 
	<%= render 'shared/errors', object: @program %> 
	<div class="field">
		<%= f.label :name %>
		<%= f.text_field :name %>
	</div>
	<div class="field">
		<%= f.label :body %>
		<%= f.text_area :body %>
	</div>
	
	<div class="actions">
		<%= f.submit %>
	</div>


<% end %>