<%= render 'shared/announcements' %>
<div class="user_index">
	<ul>
	<% @users.each do |f| %>
		<li class="user_snapshot">
			<%= link_to image_tag(profile_pic_if_uploaded(f), size: "100x100"), user_path(f) %>
			<%= link_to "#{f.first_name} #{f.last_name}", user_path(f) %><br />
			<%= mail_to f.email %><br />
			<%= f.country %><br />
			<%= f.school %><br />
		</li>
	<% end %>
	<ul>
</div>

 