$("#users_waiting_to_be_approved").html("<%= j render @users_waiting_to_be_approved %>").prepend("<h3>Users Waiting To Be Approved (<%=@users_waiting_to_be_approved.count%>)")
$("#approved_users").html("<%= j render @approved_users %>").prepend("<h3>Approved Users(<%=@approved_users.count%>)")

if (<%= @boolean %> == false) {  
	// $(".time_navigation").hide()
	$(".gray_nav_btn").show()
	$(".delete_nav_btn").hide()
}
else if (<%= @boolean %> == true) {
	// $(".time_navigation").show()
	$(".delete_nav_btn").show()
	$(".gray_nav_btn").hide()
}

